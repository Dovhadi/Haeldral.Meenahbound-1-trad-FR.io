<rooms>
  <room name='firstRoom' walkableMap='firstRoomMap' mapScale='2'>
  	<triggers>
  		<trigger>
  			<args>inBox2,char,3874,1056,4302,1141</args>
  			<action command='changeRoomRemote'>secondRoom.xml,secondRoom,559,94</action>
  		</trigger>
  		<trigger>
  			<args>inBox2,char,5116,0,5580,66</args>
  			<action command='changeRoomRemote'>thirdRoom.xml,thirdRoom,520,2544</action>
  		</trigger>
  		<trigger detonate='true'>
  			<args>spriteProperty,char,x &gt; 5300</args>
  			<action command='waitFor'>
  				<args>noActions</args>
  				<action command='addSprite' noWait='true'>
  					<args>mituna,firstRoom</args>
  					<action command='depthSprite' noWait='true'>
							<args>mituna,2</args>
							<action command='playSound' noWait='true'>
								<args>grindSound</args>
								<action command='changeFocus' noWait='true'>
									<args>mituna</args>
									<action command='deltaSprite' noWait='true' for='30'>
										<args>mituna,16,0</args>
										<action command='deltaSprite' noWait='true' for='18'>
											<args>mituna,12,-12</args>
											<action command='deltaSprite' noWait='true' for='15'>
												<args>mituna,18,18</args>
												<action command='playAnimation' noWait='true' noDelay='true'>
													<args>mituna,crash</args>
													<action command='playSound' noWait='true' noDelay='true'>
														<args>hitPowerfulSound</args>
														<action command='depthSprite' noWait='true'>
															<args>mituna,1</args>
															<action command='changeFocus' noWait='true'>
																<args>char</args>
															</action>
														</action>
													</action>
												</action>
											</action>
										</action>
									</action>
								</action>
							</action>
						</action>
  				</action>
  			</action>
  		</trigger>
  		<trigger detonate='true'>
  			<args>gameState,spadeKeyUsed=true</args>
  			<args>gameState,heartKeyUsed=true</args>
  			<args>gameState,clubKeyUsed=true</args>
  			<args>gameState,diamondKeyUsed=true</args>
  			<action command='startAnimation'>
  				<args>megaDoor,open</args>
  				<action command='removeSprite' noDelay='true'>
  					<args>spadeKeyOverlay,firstRoom</args>
  					<action command='removeSprite' noDelay='true'>
							<args>heartKeyOverlay,firstRoom</args>
							<action command='removeSprite' noDelay='true'>
								<args>clubKeyOverlay,firstRoom</args>
								<action command='removeSprite' noDelay='true'>
									<args>diamondKeyOverlay,firstRoom</args>
								</action>
							</action>
						</action>
  				</action>
  			</action>
  		</trigger>
  	</triggers>
  
  	<paths>
  		<motionpath path='firstRoomStairs1' xtox='0.7' xtoy='0.7' />
  		<motionpath path='firstRoomStairs2' xtox='0.7' xtoy='-0.7' />
  		<motionpath path='firstRoomStairs3' xtox='0.7' xtoy='0.7' />
  	</paths>
  	
  	<character class='meenah' x='427' y='458' />
  	<character class='cronus' x='2022' y='958' />
  	<sprite class='seahorsedad' x='1702' y='844' />
  	<sprite class='bicyclops' x='5173' y='627' />
  	
  	<sprite name='megaDoor' x='2124' y='644' width='100' height='60'>
  		<animation name='closed' sheet='megaDoorClosed' x='-60' y='-144'/>
  		<animation name='open' sheet='megaDoorOpen' x='-60' y='-144'/>
  	</sprite>
  	
  	<sprite class='chest1' name='firstRoomChest1' x='2250' y='700'>
  		<action command='openChest' name='Open.'>
  			<args>firstRoomChest1,spadeKey,@! you got a spade key!</args>
  			<action command='addAction'>
  				<args>megaDoor,
  					<action name='Use spade key.' command='removeAction'>
  						<args>megaDoor,Use spade key.</args>
  						<action command='setGameState'>
  							<args>spadeKeyUsed,true</args>
  							<action command='addSprite'>
  								<args>spadeKeyOverlay,firstRoom</args>
  							</action>
  						</action>
  					</action>
  				</args>
  			</action>
  		</action>
  	</sprite>
  	
  	<sprite class='chest1' name='firstRoomChest2' x='2300' y='700'>
  		<action command='openChest' name='Open.'>
  			<args>firstRoomChest2,heartKey,@! you got a heart key!</args>
  			<action command='addAction'>
  				<args>megaDoor,
  					<action name='Use heart key.' command='removeAction'>
  						<args>megaDoor,Use heart key.</args>
  						<action command='setGameState'>
  							<args>heartKeyUsed,true</args>
  							<action command='addSprite'>
  								<args>heartKeyOverlay,firstRoom</args>
  							</action>
  						</action>
  					</action>
  				</args>
  			</action>
  		</action>
  	</sprite>
  	
  	<sprite class='chest1' name='firstRoomChest3' x='2350' y='700'>
  		<action command='openChest' name='Open.'>
  			<args>firstRoomChest3,clubKey,@! you got a club key!</args>
  			<action command='addAction'>
  				<args>megaDoor,
  					<action name='Use club key.' command='removeAction'>
  						<args>megaDoor,Use club key.</args>
  						<action command='setGameState'>
  							<args>clubKeyUsed,true</args>
  							<action command='addSprite'>
  								<args>clubKeyOverlay,firstRoom</args>
  							</action>
  						</action>
  					</action>
  				</args>
  			</action>
  		</action>
  	</sprite>
  	
  	
  	<sprite class='chest1' name='firstRoomChest4' x='2400' y='700'>
  		<action command='openChest' name='Open.'>
  			<args>firstRoomChest4,diamondKey,@! you got a diamond key!</args>
  			<action command='addAction'>
  				<args>megaDoor,
  					<action name='Use diamond key.' command='removeAction'>
  						<args>megaDoor,Use diamond key.</args>
  						<action command='setGameState'>
  							<args>diamondKeyUsed,true</args>
  							<action command='addSprite'>
  								<args>diamondKeyOverlay,firstRoom</args>
  							</action>
  						</action>
  					</action>
  				</args>
  			</action>
  		</action>
  	</sprite>
  	
